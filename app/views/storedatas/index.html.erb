<h1>Listing storedatas</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Store</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @storedatas.each do |storedata| %>
  <tr>
    <td><%= storedata.name %></td>
    <td><%= storedata.email %></td>
    <td><%= storedata.store_id %></td>
    <td><%= link_to 'Show', storedata %></td>
    <td><%= link_to 'Edit', edit_storedata_path(storedata) %></td>
    <td><%= link_to 'Destroy', storedata, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Storedata', new_storedata_path %>

