<%= form_for [:admin, @storecar] do |f| %>
  <%= error_messages_for @storecar %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </div>
  <div class="field">
    <%= f.label :store_id %><br />
    <%= f.collection_select :store_id, @stores, :id , :name %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

