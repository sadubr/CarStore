<h1>Listing personcars</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Color</th>
    <th>Person</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @personcars.each do |personcar| %>
  <tr>
    <td><%= personcar.name %></td>
    <td><%= personcar.color %></td>
    <td><%= personcar.person_id %></td>
    <td><%= link_to 'Show', admin_personcar_path(personcar) %></td>
    <td><%= link_to_edit [:edit, :admin, personcar] %></td>
    <td><%= link_to_destroy [:admin, personcar] %></td>
  </tr>
<% end %>
</table>

