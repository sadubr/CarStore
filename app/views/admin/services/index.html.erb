<h1>Listing services</h1>

<table>
  <tr>
    <th>Name</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.name %></td>
    <td><%= service.user_id %></td>
    <td><%= link_to 'Show', admin_service_path(service) %></td>
    <td><%= link_to_edit [:edit, :admin, service] %></td>
    <td><%= link_to_destroy [:admin, service] %></td>
  </tr>
<% end %>
</table>

<br />

