<h1>Listing stores</h1>

<table>
  <tr>
    <th>Name</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.name %></td>
    <td><%= store.user_id %></td>
    <td><%= link_to 'Show', admin_store_path(store) %></td>
    <td><%= link_to_edit [:edit, :admin, store] %></td>
    <td><%= link_to_destroy [:admin, store] %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_admin_store_path %>

