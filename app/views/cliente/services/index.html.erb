<h1>Listing services</h1>

<table>
  <tr>
    <th>Name</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.name %></td>
    <td><%= service.user_id %></td>
    <td><%= link_to 'Show', cliente_service_path(service) %></td>
    <td><%= link_to 'Edit', edit_cliente_service_path(service) %></td>
    <td><%= link_to 'Destroy', cliente_service_path(service), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Service', new_cliente_service_path %>

