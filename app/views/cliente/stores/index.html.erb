<h1>Listing stores</h1>

<table>
  <tr>
    <th>Name</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.name %></td>
    <td><%= store.user_id %></td>
    <td><%= link_to 'Show', cliente_store_path(store) %></td>
    <td><%= link_to_edit [:edit, :cliente, store] %></td>
    <td><%= link_to 'Destroy', cliente_store_path(store), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_cliente_store_path %>

