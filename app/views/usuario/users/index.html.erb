<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.password %></td>
    <td><%= link_to 'Show', usuario_user_path(user) %></td>
    <td><%= link_to_edit [:edit, :usuario, user] %></td>
    <td><%= link_to_destroy [:usuario, user] %></td>
  </tr>
<% end %>
</table>

